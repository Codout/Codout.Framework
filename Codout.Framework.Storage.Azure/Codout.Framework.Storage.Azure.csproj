<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Title>Codout Frameword Storage Azure</Title>
		<Authors>Clovis Coli Jr</Authors>
		<Company>Codout</Company>
		<Description>Implementação da interface IStorage do Codout.Framework.Storage para uso do storage do Azure</Description>
		<Copyright>Codout</Copyright>
		<PackageProjectUrl>https://github.com/Codout/Codout.Framework</PackageProjectUrl>
		<PackageIcon>logo-nuget.png</PackageIcon>
		<RepositoryUrl>https://github.com/Codout/Codout.Framework</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>Codout;Framework;Storage;Azure;</PackageTags>
		<Version>5.0.0</Version>
		<PackageOutputPath>$(SolutionDir)Packages</PackageOutputPath>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\logo-nuget.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Azure.Storage.Blobs" Version="12.24.1" />
		<PackageReference Include="MimeTypesMap" Version="1.0.9" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Codout.Framework.Storage\Codout.Framework.Storage.csproj" />
	</ItemGroup>

</Project>
